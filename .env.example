# Vercel API Configuration
# Copy this file to .env and fill in your values

# Required: Your Vercel API token
# Get one from: https://vercel.com/account/tokens
VERCEL_API_TOKEN=

# Optional: Custom Vercel API URL (default: https://vercel.com/api)
# VERCEL_API_URL=https://vercel.com/api

# Optional: Log level (default: info)
# Options: debug, info, error, disabled
# LOG_LEVEL=info

# Optional: Time in milliseconds to wait between restore requests (default: 250)
# Increase this value if you're hitting rate limits
# RESTORE_COOLDOWN_MS=250
